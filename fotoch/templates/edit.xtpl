		var bTextbox = new AutoSuggestControl(document.getElementById("best"),document.getElementById("newbestandid"), new FotoSuggestions("ilist/gentas.php?table=bestand&retfeld=name&suchfeld=name&"));
		//var fTextbox = new AutoSuggestControl(document.getElementById("foto"),document.getElementById("newfotoid"), new FotoSuggestions("ilist/ftas.php?"));
	}
	function cclick1(checkelement){
		//alert(checkelement+"!");
		ce=document.getElementById(checkelement+"_check");
		cd=document.getElementById(checkelement+"_date");
		cu=document.getElementById(checkelement+"_user");
		if (ce.checked){
			cd.value="{heute}";
			cu.innerHTML="{user}";
		} else {
			cd.value="";
			cu.innerHTML="";
		}
		
		
	}


	function cclick2(checkelement,num,ol){
		//alert(checkelement+"!"+num);
		ce=document.getElementById(checkelement+"_"+num+"_check");
		cd=document.getElementById(checkelement+"_"+num+"_date");
		cu=document.getElementById(checkelement+"_"+num+"_user");
		cub=document.getElementById(checkelement+"_"+num+"_userbox");
		if (ce.checked){
			cd.value="{heute}";
			cu.innerHTML="{user}";
			
			if (cub!=null) cub.value=parseInt("{userid}");
			if (parseInt(num)<6){
				ce=document.getElementById(checkelement+"_"+(parseInt(num)+1)+"_check");
				ce.disabled=false;
			}
			
			//alert(checkelement+"_"+(num)+"_check");
			
			if (parseInt(num)>1){
				ce=document.getElementById(checkelement+"_"+(parseInt(num)-1)+"_check");
				ce.disabled=true;
			}
			
			//alert(checkelement+"_"+(num)+"_check");
			

		} else {
			ol=parseInt(document.getElementById(checkelement+"_olevel").value);
			
			if (parseInt(num)<=ol){
				if (!confirm("Wollen Sie den gespeicherten Status loeschen?"))
					ce=document.getElementById(checkelement+"_"+(parseInt(ol))+"_check");
					ce.checked=true;
					return(false);
			}
			cd.value="";
			cu.innerHTML="";
			if (cub!=null) cub.value=0;
			
			if (parseInt(num)<6){
				ce=document.getElementById(checkelement+"_"+(parseInt(num)+1)+"_check");
				ce.disabled=true;
			}
			if (parseInt(num)>1){
				ce=document.getElementById(checkelement+"_"+(parseInt(num)-1)+"_check");
				ce.disabled=false;
			}
			num=parseInt(num)+1;
			//alert(checkelement+"_"+(num)+"_check");
			
			
		}
		
		
	}

	  function st2init(element,level){
	  for (i=1;i<=6;i++){
	  		num=i;
			ce=document.getElementById(element+"_"+num+"_check");
			cd=document.getElementById(element+"_"+num+"_date");
			cu=document.getElementById(element+"_"+num+"_user");
			if (i-1>level){
				ce.disabled=true;
			}
		}
	  
	  }
	
	</script>

